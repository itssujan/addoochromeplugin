{
  "name": "Addoo Chrome Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Addoo Chrome Extension Desc",
  "icons": {
    "16": "img/test.png",
    "128": "img/test.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      // "scripts/chromereload.js",
      "js/background.js"
    ],
    "persistent": true

  },
  "permissions": ["activeTab","https://app.addoo.io/*"]  ,
  "page_action": {
    "default_icon": {
      "19": "img/addoo-logo.png",
      "38": "img/addoo-logo.png"
    },
    "default_title": "AddooChromePlugin",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.addoo.io/*"
      ],
      "js": [
        "js/inject.js","js/script.js"
      ]
    }
  ]
}
